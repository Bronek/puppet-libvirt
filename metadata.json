{
    "name": "gaudenz-libvirt",
    "version": "1.3.0",
    "author": "Gaudenz Steinlin",
    "license": "GPLv3",
    "summary": "Puppet module to install libvirt and create virtual domain configuration",
    "source": "http://git.cirrax.com/?p=puppet-modules/libvirt.git",
    "tags": ["libvirt", "kvm", "drbd", "virtualization"],
    "operatingsystem_support": [
